}

# a s h u em ordem ashuashuashua
Ashua(L("palavra")){
	L("comp") = strlength(L("palavra"));
	if(strequalnocase(strchar(L("palavra"), 1),"a") && strequalnocase(strchar(L("palavra"), 1),"s") && 
	strequalnocase(strchar(L("palavra"), 1),"h") && strequalnocase(strchar(L("palavra"), 1),"u"))
		L("c") = 1;
		while(L("c")<L("comp")){
			if(strequalnocase(strchar(L("palavra"), L("c")-1),"a"))
				if(strnotequalnocase(strchar(L("palavra"), L("c")),"s"))
					return 0;
			if(strequalnocase(strchar(L("palavra"), L("c")-1),"s"))
				if(strnotequalnocase(strchar(L("palavra"), L("c")),"h"))
					return 0;
			if(strequalnocase(strchar(L("palavra"), L("c")-1),"h"))
				if(strnotequalnocase(strchar(L("palavra"), L("c")),"u"))
					return 0;
			if(strequalnocase(strchar(L("palavra"), L("c")-1),"u"))
				if(strnotequalnocase(strchar(L("palavra"), L("c")),"a"))
					return 0;
			L("c")++;
		}
		return 1;
	return 0;
}

