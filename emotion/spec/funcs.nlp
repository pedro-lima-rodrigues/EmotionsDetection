# v a m em ordem e depois o
Vamo(L("palavra")){
	L("comp") = strlength(L("palavra"));
	if(strequalnocase(strchar(L("palavra"), 0),"v"))
		if(strequalnocase(strchar(L("palavra"), 1),"a"))
			if(strequalnocase(strchar(L("palavra"), 2),"m"))
				L("c") = 3;
				while(L("c")<L("comp")){
					if(strnotequalnocase(strchar(L("palavra"), L("c")),"o"))
						return 0;
					L("c")++;
				}
				return 1;
			return 0;
		return 0;
	return 0;
}

